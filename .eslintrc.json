{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "max-classes-per-file": "off",
    "import/prefer-default-export": "off",
    "no-console": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off"
  }
}
